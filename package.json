  "scripts": {
    "test": "yarn env:test ts-mocha --exit --timeout 10000 ./**/__tests__/**/*.ts",
    "start": "yarn build:tsoa && yarn env:dev ts-node-dev -r dotenv/config --respawn ./src/server.ts",
    "start:prod": "yarn build:tsoa && yarn env:prod ts-node-dev --respawn ./src/server.ts",
    "build:routes": "tsoa routes",
    "build:swagger": "tsoa swagger",
    "build:tsoa": "yarn clean && yarn build:routes && yarn build:swagger",
    "clean": "rm -rf ./dist && rm -rf ./build && mkdir build",
    "env:dev": "cross-env NODE_ENV=development",
    "env:test": "cross-env NODE_ENV=test",
    "env:prod": "cross-env NODE_ENV=production"
  },
